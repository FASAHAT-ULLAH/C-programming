#include <stdio.h>

int main() 
{
    int start_num, end_num, Sum = 0, i;

    printf("Sum of Odd No. between two numbers\n");

    printf("Enter a Number: ");
    scanf("%d", &start_num);

    printf("Enter the Ending Number: ");
    scanf("%d", &end_num);

    if (start_num > end_num)
{
        printf("Error: Starting number is higher than the Ending Number\n");
        return 1;
    }

    for (i = start_num; i <= end_num; i++) 
{
        if (i % 2 != 0) 
{
            Sum += i;
        }
    }

    printf("Sum of Odd Numbers: %d\n", Sum);

    return 0;
}
